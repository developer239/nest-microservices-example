{
  "name": "service-events",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main",
    "dev": "NODE_ENV=development nest start --watch",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --ext ts src",
    "tsc": "tsc --noEmit",
    "test": "NODE_ENV=test npx vitest --poolOptions.threads.singleThread",
    "seed-database": "npx env-cmd ts-node -r tsconfig-paths/register ./src/modules/database/seeds/run-seed.ts",
    "migration-generate": "npx env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource=src/modules/database/data-source.ts migration:generate",
    "migration-generate:named": "npx env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource=src/modules/database/data-source.ts migration:generate src/modules/database/migrations/initial",
    "migration-create": "npx env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration-run": "npx env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource=src/modules/database/data-source.ts migration:run",
    "migration-revert": "npx env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource=src/modules/database/data-source.ts migration:revert",
    "schema-drop": "npx env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource=src/modules/database/data-source.ts schema:drop"
  },
  "dependencies": {
    "@apollo/server": "4.10.4",
    "@nestjs/apollo": "12.1.0",
    "@nestjs/common": "10.3.9",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.9",
    "@nestjs/graphql": "12.1.1",
    "@nestjs/microservices": "10.3.9",
    "@nestjs/platform-express": "10.3.9",
    "@nestjs/typeorm": "10.0.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "apollo-server-express": "3.13.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "graphql": "16.8.1",
    "graphql-subscriptions": "2.0.0",
    "graphql-tools": "9.0.1",
    "joi": "17.13.1",
    "pg": "^8.11.1",
    "pg-connection-string": "^2.6.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/testing": "^10.3.9",
    "@ngneat/falso": "^7.2.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.25",
    "@types/express": "4.17.21",
    "@types/node": "20.14.1",
    "@types/pg-connection-string": "^2.0.0",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.6.0",
    "source-map-support": "0.5.21",
    "supertest": "^7.0.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "unplugin-swc": "^1.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
